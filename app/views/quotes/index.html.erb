<%= button_to "Get new quote", create_quote_path %>

<% @quotes.each do |quote| %>
  <div>
    <%= link_to "Show me the quote!!!", show_quote_path(quote) %>
    <%= button_to "Destroy!", delete_quote_path(quote), method: :delete %>
    <p><%= quote.author %></p>
    <p><%= quote.quote %></p>
    <p><%= quote.category %></p>
  </div>
<% end %>